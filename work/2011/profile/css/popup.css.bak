/* 쪽지 신고 및 차단하기 */
.popMessReport{position:absolute;border:1px solid #b1977e;width:384px;background:#fff;padding:7px;color:#5c3b1a;z-index:999999;overflow:hidden;}
.popMessReport .popWrap{padding:0 9px;}
.popMessReport .popWrap em{color:#ea4012;font-weight:bold;}
.popMessReport .popBgcolor{position:relative;background:#e9ded3;width:384px;}
.popMessReport .invite_title04{background:url(http://img.ndoors.com/webdata/profile/2010/popup/t_pop_invite.gif) 0 -137px no-repeat;width:384px;height:30px;margin-bottom:15px;}
.popMessReport .from{background:#f4eee8;padding:10px 5px;height:70px;margin-bottom:13px;}
.popMessReport .from dl{clear:both;height:20px;margin-bottom:5px;}
.popMessReport .from dt, .popMessReport .from dd{float:left;color:#7e644a;}
.popMessReport .from dt{width:50px;font-size:11px;font-weight:bold;}
.popMessReport .from dd{font-size:11px;}
.popMessReport .from dd strong{color:#ea4012;}
.popMessReport .from .line{border-bottom:1px solid #e9ded3;}
.popMessReport .not{letter-spacing:-1px;}
.popMessReport .btn{text-align:center;margin-top:10px;padding-bottom:10px;}
.popMessReport .btn .blockBtn{background:url(http://img.ndoors.com/webdata/profile/2010/popup/b_pop_block.gif) 0 0 no-repeat;width:92px;height:20px;margin-right:10px;}
.popMessReport .closed{position:absolute;right:7px;top:6px;}
.popMessReport .closed .closedBtn{background:url(http://img.ndoors.com/webdata/profile/2010/popup/b_pop_closed.gif) 0 0 no-repeat;width:14px;height:14px;}

/* 불멸광장 위클리 신청 인터뷰 보기 */
.popInterview{position:absolute; background:#fff; z-index:100;}
.popInterview .interveiw_wrap {float:left; padding:6px; border:1px solid #c2c2c2; background:#eee;}
.popInterview .interveiw {float:left; padding:20px; background:#fff;}
.popInterview .hot_info .btn01 {clear:both; margin:0 0 0 1px; padding-top:30px;}
.popInterview .hot_info .btn02 {clear:both; margin-top:8px;}
.popInterview .headinfo {float:left; margin-bottom:7px;}
.popInterview .headinfo dt {float:left; margin-right:14px; font-size:14px; color:#000; font-weight:bold;}
.popInterview .headinfo dt a {color:#000;}
.popInterview .headinfo dd {float:left; margin-top:4px; font-size:11px; color:#a7a7a7;}
.popInterview .headinfo dd .num{color:#ff7200;}
.popInterview .subinfo {clear:both; float:left; width:100%; font-size:11px; line-height:140%;}
.popInterview .subinfo dt {float:left; margin:0 3px; color:#626262; display:inline;}
.popInterview .subinfo dt.clr {clear:both;}
.popInterview .subinfo dd {float:left; color:#a7a7a7;}
.popInterview .hotmember02 {}
.popInterview .hot_interview {clear:both; float:left; width:100%;}
.popInterview .hot_interview h4  {margin:18px 0 0 0;}
.popInterview .question_wrap {clear:both; height:308px; border-bottom:1px solid #49afa4; overflow-y:scroll; overflow-x:hidden;}
.popInterview .question {clear:both; margin-bottom:12px;}
.popInterview .question dt {margin:0; padding:8px 0; font-size:12px; color:#4d4d4d; font-weight:bold; background:url(http://img.ndoors.com/webdata/bm/obt/2010/sns/line_dot.gif) left bottom repeat-x;}
.popInterview .question dd {padding:8px 3px; font-size:11px; color:#a7a7a7; border:none;}
