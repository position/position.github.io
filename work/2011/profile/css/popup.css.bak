/* 친구초대하기 팝업 */
.popInvite-01{position:absolute;border:1px solid #b1977e;width:304px;height:304px;background:#fff;padding:7px;color:#5c3b1a;z-index:999999;}
.popInvite-01 .popWrap{padding:0 9px;}
.popInvite-01 .popWrap em{color:#ea4012;font-weight:bold;}
.popInvite-01 .popBgcolor{position:relative;background:#e9ded3;width:304px;height:304px;}
.popInvite-01 .invite_title01{background:url(http://img.ndoors.com/webdata/profile/2010/popup/t_pop_invite.gif) 0 0 no-repeat;width:304px;height:30px;margin-bottom:15px;}
.popInvite-01 .alert-1{padding-bottom:15px;}
.popInvite-01 .alert-2{background:#f4eee8;width:276px;font-size:11px;color:#7e644a !important;padding:10px 5px 3px 5px;margin-bottom:15px;}
.popInvite-01 .alert-2 p{padding-bottom:3px;}
.popInvite-01 legend{color:#5c3b1a !important;padding-bottom:5px;}
.popInvite-01 .messArea{width:274px;height:83px;border:1px solid #a5917e;overflow:auto;color:#5c3b1a;padding:5px;}
.popInvite-01 .btn{text-align:center;margin-top:10px;}
.popInvite-01 .btn .invBtn{background:url(http://img.ndoors.com/webdata/profile/2010/popup/b_pop_invite.gif) 0 0 no-repeat;width:130px;height:20px;margin-right:10px;}
.popInvite-01 .btn .cancleBtn{background:url(http://img.ndoors.com/webdata/profile/2010/popup/b_pop_cancle.gif) 0 0 no-repeat;width:130px;height:20px;}
.popInvite-01 .closed{position:absolute;right:7px;top:6px;}
.popInvite-01 .closed .closedBtn{background:url(http://img.ndoors.com/webdata/profile/2010/popup/b_pop_closed.gif) 0 0 no-repeat;width:14px;height:14px;}

