/* Select */
.select{position:relative; line-height:normal; display:inline-block; *display:inline; vertical-align:middle; background:#fff; *zoom:1;z-index:99999;}
.select *{margin:0; padding:0; cursor:pointer; font:bold 12px Dotum;letter-spacing:-1px;}
.select .type01_a{border:1px solid #f44e00;}
.select .type01_b{border-top:1px solid #f44e00; border-bottom:1px solid #f44e00; }
.select .type01_c{border-left:1px solid #f44e00; border-right:1px solid #f44e00;}
.select .type01_arrow{position:absolute; top:0; right:0; width:18px; height:24px;border-left:1px solid #fff;background:url(http://img.ndoors.com/webdata/bm/obt/2010/community/i_select_arrow01.gif) center center no-repeat;width:18px; height:24px;}
.select .type02_a{border:1px solid #cdcdcd;}
.select .type02_b{border-top:1px solid #cdcdcd; border-bottom:1px solid #cdcdcd; }
.select .type02_c{border-left:1px solid #cdcdcd; border-right:1px solid #cdcdcd;}
.select .type02_arrow{position:absolute; top:0; right:0; width:18px; height:24px;border-left:1px solid #fff;background:url(http://img.ndoors.com/webdata/bm/obt/2010/community/i_select_arrow02.gif) center center no-repeat;width:18px; height:24px;}
.select .myValue{position:relative; z-index:2; left:0; top:0; color:#666; line-height:24px; _line-height:normal; text-align:left; overflow:visible; background:transparent;}
.select .myValue.selected{font-weight:bold;}
.select.open .myValue,
.select button.myValue{height:21px; width:100%; text-indent:5px; *text-indent:0; *padding-left:5px;color:#000;height:26px;}
.select div.myValue{height:30px; text-indent:8px;}
.select ul{position:absolute; top:25px; left:0; width:100%; list-style:none;background:#fff; overflow:hidden;}
.select ul.aList{display:none;}
.select.open ul.aList{display:block;}
.select ul.iList{left:-2000%;}
.select.open ul.iList{left:0;}
.select li{position:relative; overflow:hidden; white-space:nowrap; height:23px;text-align:left !important;}
.select li input.option{position:absolute; width:100%; height:20px; line-height:20px;}
.select li label{position:absolute; left:0; top:0; width:100%; text-indent:8px; *text-indent:6px; height:18px; line-height:18px; color:#767676; background:#fff;}
.select li a{display:block; text-indent:8px; *text-indent:6px; height:23px;padding-top:3px; line-height:18px; color:#000 !important; background:#fff; text-decoration:none !important;}
.select li.hover *{background:#f5f5f5;}
.selectGo{border:1px solid #bababa; height:21px; width:28px; padding:0; *margin:-1px 0 -1px 4px; overflow:visible; background:#eee; vertical-align:middle; cursor:pointer; font:bold 11px Tahoma; line-height:19px; _line-height:normal; color:#767676;}
