/* 프로필 설정 메인 */
.pfSetupSec-01{position:relative;float:left;width:248px;margin-right:30px;border:7px solid #e3e3e3;min-height:500px;}
*html .pfSetupSec-01{height:500px;}
.pfSetupSec-01 .pfSetupSec_inner{padding:9px 13px 10px 14px;}
.pfSetupSec-01 h3{position:relative;background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_setup_sec01.gif) no-repeat;width:246px;height:28px;margin:0 0 10px -25px;}
.pfSetupSec-01 h3.pho_title{background-position:0 0;}
.pfSetupSec-01 h3.char01_title{background-position:0 -37px;}
.pfSetupSec-01 h3.char02_title{background-position:0 -76px;}
.pfSetupSec-01 h3.nick_title{background-position:0 -151px;}
.pfSetupSec-01 .delegatePhoto{clear:both;height:260px;margin-bottom:20px;}
.pfSetupSec-01 .delegatePhoto .userPhoto{position:relative;width:218px;clear:both;}
.pfSetupSec-01 .delegatePhoto .userPhoto img{width:216px;height:216px;border:1px solid #6d5238;}
.pfSetupSec-01 .delegatePhoto .phoBtn{position:absolute;bottom:10px;right:10px;background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_change.png) no-repeat;width:89px;height:20px;}
.pfSetupSec-01 .delegateChar01{clear:both;margin-bottom:40px;}
.pfSetupSec-01 .delegateChar01 .char01_info{clear:both;margin-bottom:15px;}
.pfSetupSec-01 .delegateChar01 .char01_info dl{clear:both;padding-bottom:3px;}
.pfSetupSec-01 .delegateChar01 .char01_info dt, .pfSetupSec-01 .delegateChar01 dd{float:left;}
.pfSetupSec-01 .delegateChar01 .char01_info dt{font-weight:bold;color:#ea4012;padding-right:5px;}
.pfSetupSec-01 .delegateChar01 .char01_info dd{color:#616161;}
.pfSetupSec-01 .delegateChar01 .proBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_character01.gif) no-repeat;width:153px;height:20px;}
.pfSetupSec-01 .delegateChar01 .nickBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_nick_change.gif) no-repeat;width:153px;height:20px;}
.pfSetupSec-01 .delegateChar02{clear:both;margin-bottom:40px;}
.pfSetupSec-01 .delegateChar02 .char02_info{clear:both;margin-bottom:15px;}
.pfSetupSec-01 .delegateChar02 .char02_info dl{clear:both;margin-top:15px;}
.pfSetupSec-01 .delegateChar02 .char02_info dt{font-weight:bold;color:#ea4012;padding-bottom:5px;}
.pfSetupSec-01 .delegateChar02 .char02_info dd{color:#616161;}
.pfSetupSec-01 .delegateChar02 .char02_info em{font-size:11px;letter-spacing:-1px;}
.pfSetupSec-01 .delegateChar02 .gameBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_character02.gif) no-repeat;width:153px;height:20px;}

.pfSetupSec-02{position:relative;float:left;width:484px;border:7px solid #e3e3e3;min-height:500px;}
*html .pfSetupSec-02{height:500px;}
.pfSetupSec-02 .pfSetupSec_inner{padding:25px 25px 0 25px;}
.pfSetupSec-02 h4{background:#fff url(http://img.ndoors.com/webdata/profile/2010/setup/t_setup_sec02_v2.gif) no-repeat;height:22px;overflow:hidden;width:440px;}
.pfSetupSec-02 h4.my_title{background-position:0 0;}
.pfSetupSec-02 h4.sns_title{background-position:0 -42px;}
.pfSetupSec-02 h4.open_title{background-position:0 -84px;}
.pfSetupSec-02 .head{position:relative;width:440px;height:22px;}
.pfSetupSec-02 .head .btn{position:absolute;right:0;bottom:6px;}
.pfSetupSec-02 .head .btn .chgBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_change_v2.gif) no-repeat;width:66px;height:20px;}
.pfSetupSec-02 .setting_view{padding:20px 10px 30px 5px;line-height:17px;}
.pfSetupSec-02 .setting_view dfn{font-weight:bold;text-decoration:underline;color:#ea4012;line-height:18px;}
.pfSetupSec-02 .my_introduction{clear:both;position:relative;}
.pfSetupSec-02 .mySns dl{clear:both;padding-bottom:3px;}
.pfSetupSec-02 .mySns dt, .pfSetupSec-01 .mySns dd{float:left;}
.pfSetupSec-02 .mySns dt{font-weight:bold;color:#ea4012;padding-right:5px;}
.pfSetupSec-02 .mySns dd{color:#616161;}
.pfSetupSec-02 .myOpen dl{clear:both;padding-bottom:3px;}
.pfSetupSec-02 .myOpen dt, .pfSetupSec-01 .myOpen dd{float:left;}
.pfSetupSec-02 .myOpen dt{font-weight:bold;color:#ea4012;padding-right:5px;}
.pfSetupSec-02 .myOpen dd{color:#616161;}

/* 내 사진 올리기 */
.photoUpload{position:relative;width:776px;min-height:330px;border:7px solid #e3e3e3;}
.photoUpload .photoUpload_inner{padding:9px 13px 40px 14px;}
.photoUpload .preview_photo{float:left;width:218px;margin-right:20px;}
.photoUpload .preview_photo h3{position:relative;background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_setup_sec01.gif) 0 -114px no-repeat;width:246px;height:28px;margin:0 0 10px -25px;}
.photoUpload .preview_photo .userPhoto{position:relative;width:218px;clear:both;}
.photoUpload .preview_photo .userPhoto img{width:216px;height:216px;border:1px solid #6d5238;}
.photoUpload .preview_photo .userPhoto .op{display:block;position:absolute;bottom:0;left:0;width:218px;height:40px;filter:alpha(opacity=60);-moz-opacity:0.60;opacity:0.60;background:#000;color:#fff;}
.photoUpload .preview_photo .userPhoto .phoText{position:absolute;color:#b7b7b7;bottom:2px;left:10px;letter-spacing:-1px;}
.photoUpload .preview_photo .userPhoto .phoText em{color:#fff;font-size:14px;font-weight:bold;}

.photoUpload .work_photo{float:left;width:493px;}
.photoUpload .phoTab{position:relative;height:27px;margin:0 0 5px 3px;}
.photoUpload .phoTab .tabLine{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_tab_photo.gif) left bottom no-repeat;width:487px;height:27px;}
.photoUpload .phoTab li{float:left;}
.photoUpload .phoTab li a{position:relative;display:block;background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_tab_photo.gif) 0 0 no-repeat;height:27px;}
.photoUpload .phoTab li.tab_a a{background-position:0px 0px;width:136px;}
.photoUpload .phoTab li.tab_b a{background-position:-136px 0px;width:135px;}
.photoUpload .phoTab li.tab_a a.on{background-position:0px -47px;width:136px;}
.photoUpload .phoTab li.tab_b a.on{background-position:-136px -47px;width:135px;}
.photoUpload .phoTab li.tab_a a:hover{background-position:0px -47px;width:136px;}
.photoUpload .phoTab li.tab_b a:hover{background-position:-136px -47px;width:135px;}
.photoUpload .work_photo .photoBg01{background:url(http://img.ndoors.com/webdata/profile/2010/setup/k_pho_bg01.gif) 0 0 no-repeat;width:463px;height:100px;padding:15px;}
.photoUpload .work_photo .photoBg02{background:url(http://img.ndoors.com/webdata/profile/2010/setup/k_pho_bg02.gif) 0 0 no-repeat;width:453px;height:177px;padding:20px;}
.photoUpload .work_photo .photoBg01 .inp{padding-bottom:20px;}
.photoUpload .work_photo .photoBg01 .inpFile{width:440px;height:25px;}
.photoUpload .work_photo .photoBg01 p{color:#755b46;letter-spacing:-1px;padding:0 0 3px 3px;}
.photoUpload .work_photo .photoBg01 p em{font-weight:bold;color:#ea4012;}
.photoUpload .work_photo .photoBg02 .camWrap{width:450px;height:155px;margin-bottom:10px;}
.photoUpload .work_photo .photoBg02 .camView{float:left;width:216px;height:216px;background:#000;margin-right:15px;}
.photoUpload .work_photo .photoBg02 .camBtn{float:left;}
.photoUpload .work_photo .photoBg02 p{clear:both;color:#755b46;letter-spacing:-1px;padding-bottom:7px;}
.photoUpload .work_photo .photoBg02 .camBtn .shotBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_shot.gif) 0 0 no-repeat;width:97px;height:34px;}
.photoUpload .work_photo .photoBg02 .camBtn .reshotBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_reshot.gif) 0 0 no-repeat;width:97px;height:34px;}
.photoUpload .work_photo .photoBg02 .camBtn .saveBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_save.gif) 0 0 no-repeat;width:97px;height:34px;}
.pfSetupBtn{border-top:1px solid #f0ebe6;width:790px;height:40px;margin-top:20px;padding-top:10px;}
.pfSetupBtn .alert{float:left;color:#755b46;letter-spacing:-1px;padding:5px 0 0 10px;}
.pfSetupBtn .applyBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_apply.gif) 0 0 no-repeat;width:97px;height:34px;margin-right:6px;}
.pfSetupBtn .cancleBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_pho_cancle.gif) 0 0 no-repeat;width:97px;height:34px;margin-right:10px;}
.noLine{border:0 none;margin-top:10px !important;padding-top:0px !important;}


/* 대표 캐릭터 설정 */
.chracterSetting{clear:both;position:relative;width:790px;padding-top:10px;}
.chracterSetting h3{background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_character.gif) no-repeat;width:569px;}
.chracterSetting .title-01{background-position: 0 0;height:28px;}
.chracterSetting .title-02{background-position: 0 -48px;height:50px;}
.chracterSetting .chracSelect{width:770px;height:120px;background:url(http://img.ndoors.com/webdata/profile/2010/setup/i_dot.gif) left bottom repeat-x;margin-bottom:20px;}
.chracterSetting .chracSelect ul{padding-top:25px;}
.chracterSetting .chracSelect li{float:left;margin-right:70px;display:inline;}
.chracterSetting .chracSetup{height:120px;}
.chracterSetting .chracSetup dl{clear:both;padding-bottom:12px;height:30px;}
.chracterSetting .chracSetup dt{float:left;padding-top:5px;width:100px;}
.chracterSetting .chracSetup dt strong{background:url(http://img.ndoors.com/webdata/profile/2010/setup/i_arrow.gif) 0 4px no-repeat;padding-left:8px;font-weight:bold;color:#ea4012;}
.chracterSetting .chracSetup dd{float:left;margin-right:8px;display:inline;}


/* 내 소개 설정 */
.introSetting{clear:both;position:relative;width:790px;padding-top:10px;}
.introSetting h3{background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_introduction.gif) 0 0 no-repeat;width:569px;height:30px;}
.introSetting .myNickname{margin-top:30px;}
.introSetting .myNickname h4{background:url(http://img.ndoors.com/webdata/profile/2010/setup/i_arrow.gif) 0 4px no-repeat;padding-left:8px;font-weight:bold;color:#ea4012;}
.introSetting .myNickname .typer{padding:10px 0 5px 0;}
.introSetting .myNickname .nickInp{background:#f2f2f2;border:1px solid #cdcdcd;padding:4px 5px 2px 10px;width:250px;height:22px;line-height:20px;font-size:14px;font-weight:bold;color:#4f4f4f;}
.introSetting .myNickname .overBtn{background:url(http://img.ndoors.com/webdata/profile/2010/setup/b_overlap.gif) 0 0 no-repeat;width:87px;height:20px;margin-left:10px;}
.introSetting .myNickname .typ01{color:#ea7401;}
.introSetting .myNickname .typ02{color:#79a10c;}
.introSetting .myInfo{margin-top:30px;}
.introSetting .myInfo .locSel{float:left;color:#846a51;width:190px;border:1px solid #cdcdcd;margin-right:10px;}
.introSetting .myInfo h4{background:url(http://img.ndoors.com/webdata/profile/2010/setup/i_arrow.gif) 0 4px no-repeat;padding-left:8px;font-weight:bold;color:#ea4012;}
.introSetting .myInfo table{clear:both;margin-top:10px;border-top:1px solid #f0ebe6;width:776px;}
.introSetting .myInfo caption{display:none;}
.introSetting .myInfo th, .introSetting .myInfo td{height:23px;padding:7px 10px 10px 20px;}
.introSetting .myInfo th{background:#f0ebe6;width:110px;border-bottom:1px solid #fff;border-color:#fff !important;font-weight:bold;color:#ab9e91;}
.introSetting .myInfo td{color:#846a51;letter-spacing:-1px;border-right:1px solid #f0ebe6;border-bottom:1px solid #f0ebe6;}
.introSetting .myInfo td span{display:block;}
.introSetting .myInfo td p{clear:both;padding-bottom:10px;height:15px;}
.introSetting .myInfo td .answer{float:left;width:210px;padding-top:3px;}
.introSetting .myInfo td .sel{float:left;margin-right:20px;padding-left:7px;}
.introSetting .myInfo td .sel .inf-sel{font-weight:bold;border:1px solid #ddd;}
.introSetting .myInfo td .sel em{padding-left:7px;}
.introSetting .myInfo td .inp{float:right;height:20px;}
.introSetting .myInfo td .inp02{float:left;margin-left:7px;height:20px;}
.introSetting .myInfo td .inp .inf-txt{border:1px solid #cdcdcd;width:380px;height:15px;padding:4px 3px 2px 3px;}
.introSetting .myStyle{margin-top:30px;}
.introSetting .myStyle h4{background:url(http://img.ndoors.com/webdata/profile/2010/setup/i_arrow.gif) 0 4px no-repeat;padding-left:8px;font-weight:bold;color:#ea4012;}
.introSetting .myStyle table{clear:both;margin-top:10px;border-top:1px solid #f0ebe6;width:776px;}
.introSetting .myStyle caption{display:none;}
.introSetting .myStyle th, .introSetting .myStyle td{height:23px;padding:7px 10px 10px 20px;}
.introSetting .myStyle th{background:#f0ebe6;width:110px;border-bottom:1px solid #fff;border-color:#fff !important;font-weight:bold;color:#ab9e91;}
.introSetting .myStyle td{color:#846a51;letter-spacing:-1px;border-right:1px solid #f0ebe6;border-bottom:1px solid #f0ebe6;}
.introSetting .myStyle td span{display:block;}
.introSetting .myStyle td p{clear:both;padding-top:5px;height:20px;}
.introSetting .myStyle td .chk{float:left;width:250px;}
.introSetting .myStyle td .chk02{float:left;width:150px;}
.introSetting .myStyle td .radio{float:left;margin-right:10px;}


/* SNS 사이트 설정 */
.snsSetting{clear:both;position:relative;width:790px;padding-top:10px;}
.snsSetting h3{background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_snssite.gif) 0 0 no-repeat;width:569px;height:30px;margin-bottom:20px;}
.snsSetting .comment{height:40px;margin-bottom:20px;}
.snsSetting .comment p{padding-bottom:3px;}
.snsSetting .comment em{color:#ff7c00;}
.snsSetting .snsForm{clear:both;width:778px;background:url(http://img.ndoors.com/webdata/profile/2010/setup/k_website_bottom.gif) left bottom no-repeat;padding-bottom:10px;}
.snsSetting .snsFormTop{clear:both;background:url(http://img.ndoors.com/webdata/profile/2010/setup/k_website_top.gif) left top no-repeat;padding-top:10px;}
.snsSetting .snsFormCen{clear:both;background:url(http://img.ndoors.com/webdata/profile/2010/setup/k_website_center.gif) left top repeat-y;padding:10px 20px 0 25px;zoom:1;}
.snsSetting .snsForm .sns-txt{border:1px solid #cdcdcd;width:270px;height:14px;padding:4px 3px 2px 3px;}
.snsSetting .snsForm ul{clear:both;height:50px;margin-bottom:14px;}
.snsSetting .snsForm .line{border-bottom:1px solid #fff;}
.snsSetting .snsForm .mr0{margin-right:0px !important;}
.snsSetting .snsForm .mb0{margin-bottom:0px !important;}
.snsSetting .snsForm li{float:left;width:335px;margin-right:50px;}
.snsSetting .snsForm li span{display:block;float:left;}
.snsSetting .snsForm li .sort{width:150px;}
.snsSetting .snsForm li .sort em{padding-left:10px;font-weight:bold;color:#ea4012;}
.snsSetting .snsForm li .ico{float:left;width:50px;margin-right:10px;display:inline;}
.snsSetting .snsForm li .com{font-weight:bold;color:#ea4012;}


/* 공개 설정 */
.openSetting{clear:both;position:relative;width:790px;padding-top:10px;}
.openSetting h3{background:url(http://img.ndoors.com/webdata/profile/2010/setup/t_opensetup.gif) 0 0 no-repeat;width:569px;height:30px;margin-bottom:20px;}
.openSetting table th, .openSetting table td{text-align:center;height:36px;}
.openSetting table .th_col{background:#d4c6b8;font-weight:bold;color:#fff;border-right:1px solid #fff;border-bottom:1px solid #fff;}
.openSetting table .th_row{background:#f0ebe6;font-weight:bold;color:#ab9e91;border-right:1px solid #fff;border-bottom:1px solid #fff;}
.openSetting table td{border-right:1px solid #f0ebe6;border-bottom:1px solid #f0ebe6;}
.openSetting table .rb{border-right:1px solid #d4c6b8;}
.openSetting table caption{display:none;}
